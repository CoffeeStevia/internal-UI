-- Scripts:

local function XGQMJB_fake_script() -- clear.Script 
	local script = Instance.new('Script', clear)

	clear.MouseButton1Click:Connect(function()
		Source.Text = "--Cleared"
		wait(2)
		Source.Text = ""
	end)
end
coroutine.wrap(XGQMJB_fake_script)()
local function XDPMX_fake_script() -- exec.Script 
	local script = Instance.new('Script', exec)

	exec.MouseButton1Click:Connect(function()
		assert(loadstring(Source.Text))()
	end)
end
coroutine.wrap(XDPMX_fake_script)()
local function UMRKQH_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	
	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		dragSpeed = 0.15
		dragInput = nil
		dragStart = nil
		dragPos = nil
		function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	dragify(script.Parent)
	
end
coroutine.wrap(UMRKQH_fake_script)()
local function LWLCDO_fake_script() -- HideText.Script 
	local script = Instance.new('Script', HideText)

	local hidden = false
	script.Parent.MouseButton1Click:Connect(function()
		if hidden == false then
	
	
			-- Change script.Parent.Parent.Frame1.TextBox to wherever your textbox is,
			-- It could be script.Parent.Mainframe.Codebox.TextBox
			-- Change it to wherever your textbox is...
			-- Don't forget to subscribe to scrambles! :D
	
			script.Parent.Parent.ScriptBox.Transparency = 0.1
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.2
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.3
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.4
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.5
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.6
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.7
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.8
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.9
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 1
			wait()
			hidden = true
		else
			hidden = false
			script.Parent.Parent.ScriptBox.Transparency = 1
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.9
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.8
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.7
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.6
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.5
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.4
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.3
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.2
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0.1
			wait()
			script.Parent.Parent.ScriptBox.Transparency = 0
			wait()
		end	
	end)
	
end
coroutine.wrap(LWLCDO_fake_script)()
local function BSTOHI_fake_script() -- ScreenGui.Script 
	local script = Instance.new('Script', ScreenGui)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(K)
		if K == "=" then
			Main.Visible = true
		end
	end)
	
	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(K)
		if K == "-" then
			Main.Visible = false
		end
	end)
	
end

